<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration       
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"      
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	
	<typeAliases>
		<typeAlias alias="Admin" type="com.supr.blog.model.ibatis.Admin" />
		<typeAlias alias="Role" type="com.supr.blog.model.ibatis.Role" />
		<typeAlias alias="Auth" type="com.supr.blog.model.ibatis.Auth" />
		<typeAlias alias="Function" type="com.supr.blog.model.ibatis.Function" />
	</typeAliases>
	
	<mappers>
		<mapper resource="ibatis/admin.xml"/>
		<mapper resource="ibatis/role.xml"/>
		<mapper resource="ibatis/auth.xml"/>
		<mapper resource="ibatis/function.xml"/>
	</mappers>
</configuration>