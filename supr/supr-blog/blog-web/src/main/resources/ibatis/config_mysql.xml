<configuration>

	<typeAliases>
		<typeAlias alias="Admin" type="com.supr.blog.ibatis.model.Admin" />
		<typeAlias alias="Role" type="com.supr.blog.ibatis.model.Role" />
		<typeAlias alias="Auth" type="com.supr.blog.ibatis.model.Auth" />
		<typeAlias alias="Function" type="com.supr.blog.ibatis.model.Function" />
	</typeAliases>	
	
	<mappers>
		<mapper resource="config/ibatis/admin.xml"/>
		<mapper resource="config/ibatis/role.xml"/>
